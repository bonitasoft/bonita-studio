<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<theme author="bonitasoft" creationDate="1312979182074" description="The grey theme" imagePreview="preview.png" name="grey" provided="true" type="application" updateDate="1313660940461">
  <bindings>
    <binding description="The header part of the area to hold bonita form" name="containerheader">
      <css-rule>.bonita_containerheader</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="The area to display the logo" name="logo">
      <css-rule>.bonita_logo</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="To display the logo text" name="logo_content">
      <css-rule>.bonita_logo_content</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="To display the process name" name="process_label">
      <css-rule>.bonita_process_label</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="Hyperlink to the bonita User Experience" name="user_xp_link">
      <css-rule>#bonita_user_xp_link</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="To display the form page name" name="form_page_label">
      <css-rule>.bonita_form_page_label</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="To display the task candidates" name="candidates">
      <css-rule>.bonita_form_task_candidates</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="The refresh button" name="refresh">
      <css-rule>.bonita_refresh_button</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>

    <binding description="The whole html" name="html">
      <css-rule>html</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="identification area" name="identification">
      <css-rule>.bonita_identification</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="To display login user's head" name="avatar">
      <css-rule>.bonita_avatar</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="To display login user's name" name="username">
      <css-rule>.bonita_identif-1</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="To display logout button" name="logout">
      <css-rule>.bonita_identif-2</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="To display user xp link" name="user_xp_label">
      <css-rule>.bonita_user_xp_label</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="user xp icon" name="user_xp_icon">
      <css-rule>.bonita_user_xp_icon</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="To display form" name="form">
      <css-rule>div#bonita_form</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="The form table container" name="table_container">
      <css-rule>.bonita_table_container</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    
    <binding description="To display the error message" name="error_message">
      <css-rule>.bonita_form_error_message</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
   </binding>
  </bindings>
</theme>
