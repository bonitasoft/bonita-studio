<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<theme author="bonitasoft" creationDate="1312979182074" description="The black-xl theme" imagePreview="preview.png" name="black-xl" provided="true" type="application" updateDate="1313660940526"> 
  <bindings>
    <binding description="The area to hold bonita form" name="form-container">
      <css-rule>.bonita_form_container</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="To customize the HTML body tag" name="html-body">
      <css-rule>.bonita-body</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
  </bindings>
  <binding description="Hyperlink to the bonita User Experience" name="user-xp-link">
      <css-rule>.bonita_user_xp_link</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
  <binding description="To display userXP icon" name="user-xp-icon">
      <css-rule>.bonita_user_xp_icon</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
  </binding>
   <binding description="The central area to display the application" name="principal-container-middle">
      <css-rule>#bonita-principal-container-middle</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
   </binding>
   <binding description="The are to display the log" name="logo">
      <css-rule>div#bonita-bonita-open-solution-logo</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
   </binding>
   <binding description="The identification area" name="identification-area">
      <css-rule>div#bonita-identification-area</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
   </binding>
   <binding description="To display the username" name="username">
      <css-rule>.bonita_username_label</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
   </binding>
   <binding description="To display logout labal" name="logout">
      <css-rule>.bonita_logout_label, .bonita_logout_label:visited, .bonita_logout_label:hover</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
   </binding>
   <binding description="To display the whole bonita form" name="form">
      <css-rule>div#bonita_form</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
   </binding>
   <binding description="To display the form page name" name="form-page-label">
      <css-rule>.bonita_form_page_label</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
   </binding>
   <binding description="To display the bonita object area middle" name="activity-middle">
      <css-rule>div#bonita-object-area-middle</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
   </binding>
   <binding description="To display the bonita object area from" name="activity-start">
      <css-rule>div#bonita-object-area-from</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
   </binding>
   <binding description="To display the bonita object area to" name="activity-end">
      <css-rule>div#bonita-object-area-to</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
   </binding>
   <binding description="The priority of the task" name="activity-priority">
      <css-rule>.bonita-priority</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
   </binding>
   <binding description="bonita object area description" name="activity-description">
      <css-rule>div#bonita-object-area-description</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
   </binding>
   <binding description="To display the error message" name="error-message">
      <css-rule>.bonita_form_error_message</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
   </binding>
   <binding description="To display the process name" name="process-label">
      <css-rule>.bonita_process_label</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
	<binding description="To display the footer message" name="footer">
      <css-rule>.bonita-footer</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
</theme>