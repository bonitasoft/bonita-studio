<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<theme author="bonitasoft" creationDate="1363191944114" description="" imagePreview="preview.png" name="6.0 beta default" provided="true" type="application" updateDate="1313660940526">
  <bindings>
    <binding description="Hyperlink to the bonita User Experience" name="user_xp_link">
      <css-rule>.bonita_user_xp_link</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="To display the logo" name="logo">
      <css-rule>div#bonita-open-solution-logo</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="To display the process name" name="process_label">
      <css-rule>.bonita_process_label</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="To display the form area" name="form">
      <css-rule>div#bonita_form</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="The date from area" name="activity-start">
      <css-rule>div#bonita-object-area-from</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="identification area" name="identification-area">
      <css-rule>div#bonita-identification-area</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="The form table container" name="table_container">
      <css-rule>.bonita_table_container</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="To display the form page name" name="form_page_label">
      <css-rule>.bonita_form_page_label</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="The middle area to display  before form" name="activity-middle">
      <css-rule>div#bonita-object-area-middle</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="The date to area" name="activity-end">
      <css-rule>div#bonita-object-area-to</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="To display the username" name="username">
      <css-rule>.bonita_username_label</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="The form descrition area" name="activity-description">
      <css-rule>div#bonita-object-area-description</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="To display logout labal" name="logout">
      <css-rule>.bonita_logout_label, .bonita_logout_label:visited, .bonita_logout_label:hover</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="The form priority area" name="activity-priority">
      <css-rule>div#bonita-object-area-priority</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="To display the error message" name="error_message">
      <css-rule>.bonita_form_error_message</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
    <binding description="To customize the HTML body tag" name="html-body">
      <css-rule>.bonita-body</css-rule>
      <css-file>application/application/css/bonita_form_default.css</css-file>
    </binding>
  </bindings>
</theme>
