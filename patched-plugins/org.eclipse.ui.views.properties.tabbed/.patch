+                    /*
+                     * this tab is not visible
+                     */
+                    elements[i].setLayoutData(null);
+                    elements[i].setVisible(false);
+                } else {
+                    /*
+                     * this tab is visible.
+                     */
+                    //System.out.print(" visible");
