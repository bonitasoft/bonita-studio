<?xml version="1.0" encoding="UTF-8"?>
<plugin>

   <extension
         point="org.bonitasoft.studio.common.poststartup">
      <contribution
            class="org.bonitasoft.studio.team.contributions.CheckCurrentRepositoryState">
      </contribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="org.bonitasoft.studio.repository.ex.team"
            defaultHandler="org.bonitasoft.studio.team.ui.handler.ConnectToRepositoryHandler"
            id="org.bonitasoft.studio.team.ex.connect"
            name="Connect To Repository">
      </command>
      <category
            id="org.bonitasoft.studio.repository.ex.team"
            name="Team Commands">
      </category>
      <command
            categoryId="org.bonitasoft.studio.repository.ex.team"
            defaultHandler="org.bonitasoft.studio.team.ui.handler.ManageLocksHandler"
            id="org.bonitasoft.studio.repository.ex.manageLocks"
            name="Manage locks">
      </command>
      <command
            defaultHandler="org.bonitasoft.studio.team.ui.handler.ArtifactsHistoryHandler"
            id="org.bonitasoft.studio.repository.ex.history"
            name="History">
      </command>
      <command
            categoryId="org.bonitasoft.studio.repository.ex.team"
            defaultHandler="org.bonitasoft.studio.team.ui.handler.UpdateWorkspaceHandler"
            id="org.bonitasoft.studio.repository.ex.update"
            name="update">
      </command>
      <command
            categoryId="org.bonitasoft.studio.repository.ex.team"
            defaultHandler="org.bonitasoft.studio.team.ui.handler.CommitWorkspaceHandler"
            id="org.bonitasoft.studio.repository.ex.commit"
            name="commit">
      </command>
      <command
            categoryId="org.bonitasoft.studio.repository.ex.team"
            defaultHandler="org.bonitasoft.studio.team.ui.handler.ManageSharingTypeRepositoryHandler"
            id="org.bonitasoft.studio.repository.ex.manageshare"
            name="Manage share">
      </command>
      <command
            categoryId="org.bonitasoft.studio.repository.ex.team"
            defaultHandler="org.bonitasoft.studio.team.ui.handler.CreateRestorePointHandler"
            id="org.bonitasoft.studio.team.ex.backup"
            name="Create Restore Point">
      </command>
      <command
            defaultHandler="org.bonitasoft.studio.team.ui.handler.RepositoryStatusHandler"
            id="org.bonitasoft.studio.repository.status"
            name="status">
      </command>
      <command
            defaultHandler="org.bonitasoft.studio.team.ui.handler.CreateNewLocalRepoHandler"
            id="org.bonitasoft.studio.repository.ex.createLocal"
            name="%createNewLocalRepository">
      </command>
      <command
            defaultHandler="org.bonitasoft.studio.team.ui.handler.SwitchRepositoriesWorkspaceHandler"
            id="org.bonitasoft.studio.repository.switch"
            name="%Change_Repository">
      </command>
      <command
            defaultHandler="org.bonitasoft.studio.team.ui.handler.ShowRepositoryInSystemExplorerHandler"
            id="org.bonitasoft.studio.team.command.showinexplorer"
            name="Show in system explorer">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.keywords">
      <keyword
            id="org.bonitasoft.studio.repository.ex.keyword.svn"
            label="repository,svn,connector,team">
      </keyword>
   </extension>
   <extension
         point="org.bonitasoft.studio.preferences.preferenceFieldEditorContribution">
      <fieldEditor
            class="org.bonitasoft.studio.team.ui.preferences.SVNConnectorFieldEditorContribution">
      </fieldEditor>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.bonitasoft.studio.team.preferences.TeamPreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.team.core.fileTypes">
      <fileTypes
            extension="proc"
            type="text">
      </fileTypes>
      <extension
            point="org.eclipse.team.core.ignore">
         <ignore
               enabled="true"
               pattern="provided-libs">
         </ignore>
         <ignore
               enabled="true"
               pattern="bin">
         </ignore>
         <ignore
               enabled="true"
               pattern="DefaultUserScript.groovy">
         </ignore>
         <ignore
               enabled="true"
               pattern=".settings">
         </ignore>
         <ignore
               enabled="true"
               pattern="providedscripts">
         </ignore>
         <ignore
               enabled="true"
               pattern=".classpath">
         </ignore>
         <ignore
               enabled="true"
               pattern="Provided">
         </ignore>
      </extension>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.bonitasoft.studio.application.project.explorer?after=additions">
         <command
               commandId="org.bonitasoft.studio.repository.switch"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activeMenuSelection">
                  <and>
                     <count
                           value="1">
                     </count>
                     <or>
                        <iterate>
                           <instanceof
                                 value="org.eclipse.core.resources.IProject">
                           </instanceof>
                           <test
                                 property="org.bonitasoft.studio.application.isBonitaProject">
                           </test>
                        </iterate>
                     </or>
                  </and>
               </with>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:org.bonitasoft.studio.fileMenu?after=org.bonitasoft.studio.exporter.exportAs">
         <separator
               name="org.bonitasoft.studio.team.separator2"
               visible="true">
         </separator>
         <command
               commandId="org.bonitasoft.studio.repository.switch"
               id="org.bonitasoft.studio.repository.switch.menu"
               style="push">
         </command>
         <separator
               name="org.bonitasoft.studio.team.separator3"
               visible="true">
         </separator>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:org.bonitasoft.studio.fileMenu?before=org.bonitasoft.studio.application.command.deployArtifacts">
         <command
               commandId="org.bonitasoft.studio.repository.ex.createLocal"
               style="push">
         </command>
         <separator
               name="org.bonitasoft.studio.team.separator1"
               visible="true">
         </separator>
      </menuContribution>
   </extension>
   <extension
         point="org.bonitasoft.studio.repositoryFactory">
      <repositoryFactory
            class="org.bonitasoft.studio.team.repository.RepositoryFactory"
            priority="0">
      </repositoryFactory>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            sequence="CTRL+ALT+C"
            contextId="org.bonitasoft.studio.context.id"
            commandId="org.bonitasoft.studio.repository.ex.commit"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension
         point="org.bonitasoft.studio.common.repository.lockStatusProvider">
      <lockStatusProvider
            class="org.bonitasoft.studio.team.core.SVNLockManager">
      </lockStatusProvider>
   </extension>
   <extension
         point="org.eclipse.team.svn.core.resourceIgnoreRules">
      <resourceIgnoreRules
            class="org.bonitasoft.studio.team.core.SVNIgnoreRecommendations">
      </resourceIgnoreRules>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.bonitasoft.studio.team.ui.ComparePerspectiveFactory"
            id="org.bonitasoft.studio.team.compare.perspective"
            name="Comapre perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.commandImages">
      <image
            commandId="org.bonitasoft.studio.repository.switch"
            icon="icons/change-repository.png">
      </image>
      <image
            commandId="org.bonitasoft.studio.repository.ex.createLocal"
            icon="icons/new-local-repository.png">
      </image>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <actionProvider
            class="org.bonitasoft.studio.team.ui.provider.ProjectActionProvider"
            id="org.bonitasoft.studio.team.actionProvider">
         <enablement>
         </enablement>
      </actionProvider>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewerActionBinding
            viewerId="org.bonitasoft.studio.application.project.explorer">
         <includes>
            <actionExtension
                  pattern="org.bonitasoft.studio.team.actionProvider">
            </actionExtension>
         </includes>
      </viewerActionBinding>
   </extension>
   <extension
         id="org.bonitasoft.studio.team.fragment"
         point="org.eclipse.e4.workbench.model">
      <fragment
            uri="fragment.e4xmi"
            apply="always">
      </fragment>
   </extension>

</plugin>
