      <categoryActivityBinding
            activityId="org.bonitasoft.studio.profiles.run.menu"
            categoryId="org.bonitasoft.studio.profiles.execution">
      </categoryActivityBinding>
      <categoryActivityBinding
            activityId="org.bonitasoft.studio.profiles.dependencies.menu"
            categoryId="org.bonitasoft.studio.profiles.dependencies">
      </categoryActivityBinding>
      <categoryActivityBinding
            activityId="org.bonitasoft.studio.profiles.validation.activity"
            categoryId="org.bonitasoft.studio.profiles.validation">
      </categoryActivityBinding>
      <categoryActivityBinding
            activityId="org.bonitasoft.studio.profiles.connector.menu"
            categoryId="org.bonitasoft.studio.profiles.connectors">
      </categoryActivityBinding>
      <categoryActivityBinding
