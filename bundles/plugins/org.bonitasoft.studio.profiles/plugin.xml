<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         name="Categories"
         point="org.eclipse.ui.activities">
      <category
            id="org.bonitasoft.studio.profiles.processmodeling"
            name="Process Modeling">
      </category>

      <category
            id="org.bonitasoft.studio.profiles.formsmodeling"
            name="Forms Modeling">
      </category>
      <category
            id="org.bonitasoft.studio.profiles.appearance"
            name="Appearance Customization">
      </category>
      <category
            id="org.bonitasoft.studio.profiles.validation"
            name="Validation">
      </category>
      <category
            id="org.bonitasoft.studio.profiles.dependencies"
            name="Dependencies Management">
      </category>
      <category
            id="org.bonitasoft.studio.profiles.execution"
            name="Execution/Deployment">
      </category>
      <category
            id="org.bonitasoft.studio.profiles.connectors"
            name="Connectors">
      </category>
      <category
            id="org.bonitasoft.studio.profiles.datamanagement"
            name="Data Management">
      </category>
