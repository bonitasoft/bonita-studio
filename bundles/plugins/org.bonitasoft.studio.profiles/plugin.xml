      </categoryActivityBinding>
      <categoryActivityBinding
            activityId="org.bonitasoft.studio.profiles.looknfeel.tab"
            categoryId="org.bonitasoft.studio.profiles.looknfeels">
      </categoryActivityBinding>
      <categoryActivityBinding
            activityId="org.bonitasoft.studio.profiles.execution.view"
            categoryId="org.bonitasoft.studio.profiles.execution">
      </categoryActivityBinding>
      <categoryActivityBinding
            activityId="org.bonitasoft.studio.profiles.data.view"
            categoryId="org.bonitasoft.studio.profiles.datamanagement">
      </categoryActivityBinding>
      <categoryActivityBinding
            activityId="org.bonitasoft.studio.profiles.uidesigner.menu"
            categoryId="org.bonitasoft.studio.profiles.formsmodeling">
      </categoryActivityBinding>
   </extension>
     <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.bonitasoft.studio.profiles.ProfilePreferenceInitializer">
      </initializer>
   </extension>
     <extension
           point="org.bonitasoft.studio.repositoryStore">
        <repositoryStore
              class="org.bonitasoft.studio.profiles.repository.ProfileRepositoryStore">
        </repositoryStore>
     </extension>
     <extension
           point="org.eclipse.core.expressions.propertyTesters">
        <propertyTester
              class="org.bonitasoft.studio.profiles.manager.ActivityPropertyTester"
              id="org.eclipse.ui.activityPropertyTester"
              namespace="org.bonitasoft"
              properties="isActivityEnabled,isCategoryEnabled,is6XLegacyEnabled"
              type="org.eclipse.ui.IWorkbenchWindow">
        </propertyTester>
     </extension>

</plugin>
